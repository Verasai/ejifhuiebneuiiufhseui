Barraging2 = false
function UltraBarrage2()
	PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Barrage2)
	duration = .2
	speed = PunchAnim.Length / duration
	lenght = duration / PunchAnim.Length
	print(duration / PunchAnim.Length)
	Barraging2 = true



	PunchAnim:Play()
	PunchAnim:AdjustSpeed(2)

	repeat wait(0.025)


		PlaySoundEffect(Player.Character.Humanoid, 6477407899, 10, 1.035)
		rStorage.Damage11Sans:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame * CFrame.new(math.random(-3,3),math.random(-3,3),math.random(-3,3)), 99, 0, Vector3.new(0,0,0), 0.015, "rbxassetid://545219984", 1, 0)

		a = math.random(1,3)
		if a == 1 then
			barragepart12()
		elseif a == 2 then
			barragepart22()
		elseif a == 3 then
			barragepart32()
		end
	until Barraging2 == false
	PunchAnim:Stop()
end
Mouse.KeyDown:Connect(function(ke)
	if death == true then return end

	if ke == "e" then
		if 	Mode2 == true then  

			UltraBarrage2()
		end
	end
end)
Mouse.KeyUp:Connect(function(ke)
	if death == true then return end

	if ke == "e" then
		if 	Mode2 == true then  

			Barraging2 = false
			Barraging = false
		end
	end
end)
Mouse.KeyDown:Connect(function(ke)
	if death == true then return end

	if ke == "1" then
		if 	Mode2 == true then  
			Mode2 = false
		elseif Mode2 == false then
			Mode2 = true
		end
	end
end)

function ExplosionReal()
	hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
	hitbox1.CFrame = game.Players.LocalPlayer.Character["Left Arm"].CFrame
	hitbox1.Name = "hitbox1"
	hitbox1.Transparency = 1
	hitbox1.Anchored = false
	hitbox1.CanCollide = false


	weld = Instance.new("WeldConstraint", hitbox1)
	weld.Part0 = hitbox1
	weld.Part1 = game.Players.LocalPlayer.Character["Left Arm"]

	hitbox1.Size = game.Players.LocalPlayer.Character["Left Arm"].Size * 1.5
	hitbox1.Massless = true
	hitbox1.Color = Color3.new(0,0,0)
	game.Debris:AddItem(hitbox1, 0.5)  



	debounce = false

	hitRegister = hitbox1.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				speed = 0.1

				debounce = true
				print(hit.Parent.Name)
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				rStorage.Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 99, 1, Vector3.new(0,50,0), 0.15, "rbxassetid://545219984", 1, 10000)
				wait(1)
				rStorage.Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 99, 1, Vector3.new(0,50,0), 0.15, "rbxassetid://545219984", 1, 1000)
				wait(0.75)
				game.ReplicatedStorage.Anchor:FireServer(hit.Parent.HumanoidRootPart,true)
				targetcframe = hit.Parent.HumanoidRootPart.CFrame
				AxeKick = Instance.new("Animation")
				AxeKick.AnimationId = "rbxassetid://5171403736"

				AxeKickTrack = BodyParts.Humanoid:LoadAnimation(AxeKick)
				AxeKickTrack:Play()
				AxeKickTrack:AdjustSpeed(3)
				game:GetService('TweenService'):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(speed), {CFrame = targetcframe* CFrame.new(0,0,3.5)}):Play()
				wait(.25)
				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, math.huge, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)

				rStorage.Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 99, 1, Vector3.new(0,50,0), 0.15, "rbxassetid://545219984", 1, 10000)
				hitbox1:Destroy()
				game.ReplicatedStorage.Anchor:FireServer(hit.Parent.HumanoidRootPart,false)

			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				debounce = true
				print(hit.Parent.Name .. " elif")
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)

				rStorage.Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 99, 1, Vector3.new(0,0,0), 0.15, "rbxassetid://545219984", 1, 1000)
				wait(0.75)
				game.ReplicatedStorage.Anchor:FireServer(hit.Parent.HumanoidRootPart,true)

				targetcframe = hit.Parent.HumanoidRootPart.CFrame
				AxeKick = Instance.new("Animation")
				AxeKick.AnimationId = "rbxassetid://5171403736"

				AxeKickTrack = BodyParts.Humanoid:LoadAnimation(AxeKick)
				AxeKickTrack:Play()
				AxeKickTrack:AdjustSpeed(3)

				game:GetService('TweenService'):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(speed), {CFrame = targetcframe * CFrame.new(0,0,3.5)}):Play()
				wait(.25)
				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, math.huge, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)

				rStorage.Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 99, 1, Vector3.new(0,0,0), 0.15, "rbxassetid://545219984", 1, 10000)
				hitbox1:Destroy()
				game.ReplicatedStorage.Anchor:FireServer(hit.Parent.HumanoidRootPart,false)


			end
		end
	end)
end
function ExplosionRealism()
	local UpperCutz = Player.Character.Humanoid:LoadAnimation(AnimationTable.UpperCut)
	UpperCutz:Play()
	UpperCutz:AdjustSpeed(1.75)
	wait(.25)
	ExplosionReal()
end
Mouse.KeyDown:Connect(function(ke)
	if death == true then return end

	if ke == "r" then
		if 	Mode2 == true then  
			ExplosionRealism()
		end
	end
end)

 

LolPose = false
function LePos()
	LolPose = true
 	PlayAudioRewrited(1804093625,1,100000000,true,0,false)
	Player.Character.Humanoid.WalkSpeed = 0
	Player.Character.Humanoid.JumpPower = 0
	Pose = Player.Character.Humanoid:LoadAnimation(AnimationTable.MenacingPose)
	Pose:Play()
	rStorage.RTZ:FireServer(false)
	wait(1.3)
	Pose:AdjustSpeed(0)

	repeat 
		wait()
 
		 
		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, hrp.CFrame* CFrame.new(0,-40,0), 0, 0, 0, 0.65, "", 1, 0)
		wait()
		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, hrp.CFrame * CFrame.new(40,-40,0), 0, 0, 0, 0.65, "", 1, 0)
				wait()

		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, hrp.CFrame * CFrame.new(-40,-40,0), 0, 0, 0, 0.65, "", 1, 0)
				wait()

		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, hrp.CFrame * CFrame.new(0,-40,40), 0, 0, 0, 0.65, "", 1, 0)
				wait()

		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, hrp.CFrame * CFrame.new(0,-40,-40), 0, 0, 0, 0.65, "", 1, 0)
				wait()


	until LolPose == false
	if eas == true then
		rStorage.RTZ:FireServer(false)
	end
	Character.Humanoid.WalkSpeed = 16
	Character.Humanoid.JumpPower = 50

	Pose:Stop()



end
Mouse.KeyDown:Connect(function(key)
 
	if key == "p" then

		if Mode2 == true then
			if LolPose == false then
				LePos()
			elseif LolPose == true then
				LolPose = false
				StopSound(1804093625)
			end
		end
	end
end)
function SaiyanCumbo(Target)
	local PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Barrage)
	duration = .2
	speed = PunchAnim.Length / duration
	lenght = duration / PunchAnim.Length
	print(duration / PunchAnim.Length)
	Barraging = true

	coroutine.resume(coroutine.create(function()
		repeat

			wait(duration / PunchAnim.Length)
			PunchAnim:Play()
			PunchAnim:AdjustSpeed(PunchAnim.Length / duration)
		until Barraging == false
	end))
	for i = 1,100 do wait()

		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Target.HumanoidRootPart.CFrame * CFrame.new(math.random(-5,5),0,math.random(-5,5))
		rStorage.Damage3:FireServer(Target.Humanoid, Target.HumanoidRootPart.CFrame, 7, 1, Vector3.new(0,0,0), 0.15, "rbxassetid://545219984", 1, 1000)
	end
	Barraging = false
	PunchAnim:Stop()
end
Mouse.KeyDown:Connect(function(key)
	if key == "u" then
		if 	Mode2 == true then    

			if Mouse.Target and Mouse.Target.Parent then
				Character = nil
				if Mouse.Target.Parent:IsA("Model") and Mouse.Target.Parent.Name == "Stand" then
					Character = Mouse.Target.Parent.Parent
				elseif Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") then
					Character = Mouse.Target.Parent
				end
				if Character then
					SaiyanCumbo(Character)
				end
			end
		end
	end
end)
local stopblasting = false
function blaster()

	local RecordPoint = BodyParts.HumanoidRootPart.Position
	stopblasting = true
	AxeKick = Instance.new("Animation")
	AxeKick.AnimationId = "rbxassetid://3681052896"

	AxeKickTrack = BodyParts.Humanoid:LoadAnimation(AxeKick)
	AxeKickTrack:Play()
	wait(1)
	AxeKickTrack:AdjustSpeed(0)
	repeat wait(.3)
		coroutine.resume(coroutine.create(function()
			local LegPosition = game.Players.LocalPlayer.Character["Right Arm"].Position
			local HealthObj = Instance.new("Part")
			HealthObj.Anchored = true
			HealthObj.CanCollide = false
			HealthObj.Transparency = 1                               
			HealthObj.Size = Vector3.new(30,30,30)
			HealthObj.Position = LegPosition
			HealthObj.Parent = Character
			local pe = mouse.Hit.p
			local a = math.random(-18,20)
			local a2 = math.random(-18,20)
			local a3 = math.random(-18,20)
			for Indx2 = 0,1,.025 do
				HealthObj.Position = Quadatric(Indx2,LegPosition,(HealthObj.Position+LegPosition)/2 + Vector3.new(a2,a,a3),pe)
				Remotes.Damage:FireServer(BodyParts.Humanoid,HealthObj.CFrame,0,.1,Vector3.new(0,0,0),"rbxassetid://0",1,Color3.new(1,1,1),"rbxassetid://1091083826",0,0)
				Run.Heartbeat:Wait()
			end

			rStorage.Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, HealthObj.CFrame, 0, 0, Vector3.new(0,0,0), 0.15, "rbxassetid://545219984", 1, 10000)

			hitRegister = HealthObj.Touched:Connect(function(hit)
				if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
					rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, math.huge, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)

				end

				HealthObj:Destroy()



			end)

		end))
	until stopblasting == false
	AxeKickTrack:Stop()
end
Mouse.KeyDown:Connect(function(Key)
	if death == true then return end

	if Key == "h" then
		if Mode2 == true then
			blaster()
		end
	end
end)
Mouse.KeyUp:Connect(function(Key)
	if death == true then return end

	if Key == "h" then
		if Mode2 == true then
			stopblasting = false
		end
	end
end)
local stoped = false
function edgyaura()
	repeat wait()
		nonoMainObj.CFrame = hrp.CFrame * CFrame.new(0,-40,0)
		NotMainObj.CFrame = hrp.CFrame * CFrame.new(40,-40,0)
		NotMainObj2.CFrame = hrp.CFrame * CFrame.new(-40,-40,0)
		NotNotMainObj.CFrame = hrp.CFrame * CFrame.new(0,-40,40)
		NotNotMainObj2.CFrame = hrp.CFrame * CFrame.new(0,-40,-40)
		game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotNotMainObj2.CFrame, 0, 0, nil, 0.65, "", 1, 0)
		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotNotMainObj.CFrame, 0, 0, nil, 0.65, "", 1, 0)
		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotMainObj2.CFrame, 0, 0, nil, 0.65, "", 1, 0)
		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotMainObj.CFrame, 0, 0, nil, 0.65, "", 1, 0)
		game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, nonoMainObj.CFrame, 0, 0, nil, 0.65, "", 1, 0)
	until stoped
end
if rStorage:FindFirstChild("CommandClean") then
	rStorage.CommandClean:FireServer()
end
coroutine.resume(coroutine.create(function()
	while Player.Character.Humanoid.Health > 0.01 do
		wait()
		if Player.Character.Humanoid.PlatformStand == true and GetUp == false then
			game:GetService("ReplicatedStorage").GetUp:FireServer()
			Player.Character.Humanoid:SetStateEnabled(3, true)
			Player.Character.Humanoid.PlatformStand = false
			Player.Character.Humanoid.AutoRotate = true
			GetUp = true
		end
		if math.random(1, 4) == 1 then
			GetUp = false
		end
	end
end))
-- (RTZ) Counter
rStorage.RTZClient.OnClientEvent:connect(function(GivenPlayer)
	if GivenPlayer ~= Player then
		Humanoid = GivenPlayer.Character.Humanoid
		esa = math.random(1,2)
		if esa == 1 then
			ass1 = Player.Character.Humanoid:LoadAnimation(AnimationTable.uia)
			ass1:Play()
		elseif esa == 2 then
			ass2 = Player.Character.Humanoid:LoadAnimation(AnimationTable.uib)
			ass2:Play()

		end


	end
end)

