Model.Parent = chr
Size = 0
Size2 = 0
function SpiritBall() 

	Model.Parent = chr


	game.Players.LocalPlayer.Character.Humanoid.HipHeight = 20

	bangAnim = Instance.new("Animation")
	bangAnim.AnimationId = "rbxassetid://148840371"
	bang = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(bangAnim)
	bang:Play()
	bang:AdjustSpeed(0)
	--3445901732
	wait(2)
	knife = Instance.new("Part")
	knife.Anchored = true
	knife.CanCollide = false
	knife.Massless = true
	knife.Transparency = 1
	knife.Size = Vector3.new(150,150,150)
	knife.Parent = Model
	knife.Position = Player.Character.HumanoidRootPart.Position + Vector3.new(0,20,0)


	coroutine.resume(coroutine.create(function()
		ProjectileAuraSetter = 0

		repeat
			wait()
			ProjectileAuraSetter = ProjectileAuraSetter + 1
			--   game.ReplicatedStorage.Damage12Enderman:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*11.25, math.cos(ProjectileAuraSetter * 50)*11.25, math.sin(ProjectileAuraSetter * 75)*11.25) , 0, 0, nil, 0.5, "rbxassetid://2835079178", 1, 0)
			game.ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*21.25, math.cos(ProjectileAuraSetter * 50)*21.25, math.sin(ProjectileAuraSetter * 75)*21.25), 1, 0,Vector3.new(0, 0, 0), "rbxassetid://", 5, Color3.new(255, 255, 255), "rbxassetid://367499850", 0, 0)
			game.ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*-21.25, math.cos(ProjectileAuraSetter * 50)*-21.25, math.sin(ProjectileAuraSetter * 75)*-21.25), 1, 0,Vector3.new(255, 255, 255), "rbxassetid://", 5, Color3.new(255, 255, 255), "rbxassetid://367499850", 0, 0)

			game.ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(0,0,0), 1, 0,Vector3.new(0, 0, 0), "rbxassetid://", 10, Color3.new(0, 0,0), "rbxassetid://367499850", 0, 0)
		until Model.Parent ~= Player.Character


	end))
	wait(5)
	knife.CFrame = CFrame.lookAt(knife.Position, Mouse.Hit.p)
	knife.Anchored = false
	boopyve = Instance.new("BodyVelocity")
	boopyve.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
	boopyve.P = math.huge
	boopyve.Velocity = knife.CFrame.lookVector * 50
	boopyve.Parent = knife
	boopyve.Name = "Force"

	coroutine.resume(coroutine.create(function()
		game.Debris:AddItem(knife, 15)
		wait(15)
		knife:Destroy()
		Model.Parent = game.Workspace
	end))
	game.Players.LocalPlayer.Character.Humanoid.HipHeight = 0
	bang:Stop()
	bangAnimer = Instance.new("Animation")
	bangAnimer.AnimationId = "rbxassetid://3445901732"
	banger = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(bangAnimer)
	banger:Play()


	debounce = false

	-- deal damage if arm hits a humanoid
	hitRegister = knife.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.HamonSword:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))


			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.HamonSword:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))


			end
		end
	end)
end
function SmashHitBox()
	for i,v in pairs(game.Workspace.Entities:GetChildren()) do
		if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
			if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 70 then
				game.ReplicatedStorage.HamonSword:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
				rStorage.BurnDamage:FireServer(v, v.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
				game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 1, "", 1, 1)
			end
		end
	end
end
local AuraSetter = 0
function Smash()
	local Smashe = Player.Character.Humanoid:LoadAnimation(AnimationTable.Smash)
	Smashe:Play()
	wait(.5)
	Smashe:AdjustSpeed(0)

	repeat
		wait()
		AuraSetter = AuraSetter + 1


		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*1, math.sin(AuraSetter * 50)*0), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*2, math.sin(AuraSetter * 50)*1), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://", 3, Color3.new(191, 183, 177), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*-1, math.sin(AuraSetter * 50)*0), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*-2, math.sin(AuraSetter * 50)*-2), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://", 3, Color3.new(191, 183, 177), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*-2, math.sin(AuraSetter * 50)*2), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)
	until AuraSetter >= 100
	Smashe:AdjustSpeed(2)
	wait(0.05)
	AuraSetter = 0
	game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-2,-2,-2), Vector3.new(-1,-1,-1), BrickColor.new("Black"))
	game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-3,-3,-3), Vector3.new(-2,-1,-2), BrickColor.new("Black"))
	game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-3,-4,-3), Vector3.new(-3,-1,-3), BrickColor.new("Black"))
	game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-4,-1,-4), Vector3.new(-4,-1,-4), BrickColor.new("Black"))

	game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-.1,-10,-10), Vector3.new(-.1,-10,-10), BrickColor.new("Black"))
	game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-10,-10,-.1), Vector3.new(-10,-10,-.1), BrickColor.new("Black"))

	PlaySoundEffect(Player.Character.Humanoid, 1202657035, 76, 2)


	SmashHitBox()

end
tossactive = false
