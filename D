cani = false
function suss(Target)
	Kick = Player.Character.Humanoid:LoadAnimation(AnimationTable.Kick)
	Bonk = Player.Character.Humanoid:LoadAnimation(AnimationTable.Bonk)
	rStorage.FOTPSDamageTeleport:FireServer(Target, 15)
	cani = true
	PlayAudioRewrited(6531993802,1,111,false,0.3,false)

	wait(2.2)
	StopSound(6531993802)
	Player.Character:SetPrimaryPartCFrame(Target.Torso.CFrame * CFrame.new(0, 1.25, 3.65))
	wait()
	rStorage.Damage3:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame, 1e-200, 1, Vector3.new(0,100,0), 0.06, "rbxassetid://6986412512", 1, 100)
	wait(1.5)
	rStorage.Anchor:FireServer(Target["HumanoidRootPart"],true)
	for i = 1,5 do
		wait()
		Player.Character:SetPrimaryPartCFrame(Target.Torso.CFrame * CFrame.new(0, 1.25, 3.65))
	end
	wait()
	Player.Character.HumanoidRootPart.Anchored = true

	coroutine.resume(coroutine.create(function()
		repeat wait(.1)
			Kick:Play()
			Kick:AdjustSpeed(3) 
		until cani == false
	end))
	coroutine.resume(coroutine.create(function()
		repeat wait(.1)
			rStorage.Anchor:FireServer(Target.HumanoidRootPart,true)
			rStorage.Damage3:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame, 1e-200, 0, Vector3.new(0,0,0), 0.06, "rbxassetid://6986412512", 1, 100)
		until cani == false
	end))
	wait(3)
	Kick:Stop()
	cani = false
	Bonk:Play()
	wait(0.75)
	rStorage.Anchor:FireServer(Target.HumanoidRootPart,false)
	wait()
	coroutine.resume(coroutine.create(function()
		for i = 1, 10 do
			rStorage.BurnDamage:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame, 9e999, 3, Target.Torso.CFrame:Inverse().lookVector * 0 * Vector3.new(0, -999, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://6986412512", 1, 75)
			rStorage.Damage12:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame * CFrame.new(0, -5, 0), 99, 3, Vector3.new(-0,-998,-0) * 9e999, 1e-320, "rbxassetid://6986412512", 1, 10)
			rStorage.Damage3:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame * CFrame.new(0, -5, 0), 99, 1, Vector3.new(0, -999, 0), 1e-320, "rbxassetid://6986412512", 1, 10)
		end
	end))	  
	Player.Character.HumanoidRootPart.Anchored = false
end
