Stop = function() Break = true wait() Break = false end
Stop2 = function() BreakF = true wait() BreakF = false end
Stop3 = function() BreakR = true wait() BreakR = false end
Stop4 = function() BreakQ = true wait() BreakQ = false end
ZZ = function(T) return math.acos(math.cos(T * math.pi)) / math.pi end
Quadatric = function(T,P0,P1,P2)
	P0P1 = P0:Lerp(P1,T)
	P1P2 = P1:Lerp(P2,T)
	return P0P1:Lerp(P1P2,T)
end
Players = game.Players
function t()

	GlobalDebounce = true
	T[1] = true
	RecordPoint = BodyParts.HumanoidRootPart.Position
	AxeKick = Instance.new("Animation")
	AxeKick.AnimationId = "rbxassetid://3445902956"

	AxeKickTrack = BodyParts.Humanoid:LoadAnimation(AxeKick)
	AxeKickTrack:Play()
	wait(.4)
	coroutine.resume(coroutine.create(function()
		AxeKickTrack:AdjustSpeed(0)
		wait(.25)
		AxeKickTrack:Stop(.3)
	end))
	for Indx,Playerer in pairs(Players:GetPlayers()) do
		if Playerer ~= LocalPlayer then
			PlayerCharacter = Playerer.Character
			PlayerHumanoid = PlayerCharacter:FindFirstChildOfClass("Humanoid")
			Magnitude = (RecordPoint-PlayerCharacter.HumanoidRootPart.Position).Magnitude
			if Magnitude <= math.huge then
				coroutine.resume(coroutine.create(function()
					LegPosition = BodyParts.RightArm.Position
					HealthObj = Instance.new("Part")
					HealthObj.Anchored = true
					HealthObj.CanCollide = false
					HealthObj.Transparency = 1
					HealthObj.Size = Vector3.new(0,0,0)
					HealthObj.Position = LegPosition
					HealthObj.Parent = Character
					for Indx2 = 0,1,.025 do
						HealthObj.Position = Quadatric(Indx2,LegPosition,(PlayerCharacter.HumanoidRootPart.Position+LegPosition)/2 + Vector3.new(0,Magnitude,0),PlayerCharacter.HumanoidRootPart.Position)
						Remotes.Damage:FireServer(BodyParts.Humanoid,HealthObj.CFrame,0,.1,Vector3.new(0,0,0),"rbxassetid://0",10,Color3.new(0,1,0),"rbxassetid://1091083826",0,0)
						Run.Heartbeat:Wait()
					end
					HealthObj:Destroy()

					-- game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(PlayerHumanoid, PlayerCharacter.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
					--game:GetService("ReplicatedStorage").Damage12:FireServer(PlayerHumanoid, PlayerCharacter.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)

					rStorage.VampireRush:FireServer(PlayerHumanoid, PlayerCharacter.HumanoidRootPart.CFrame, -(PlayerHumanoid.MaxHealth - PlayerHumanoid.Health), 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
					Remotes.Damage:FireServer(BodyParts.Humanoid,PlayerCharacter.HumanoidRootPart.CFrame,0,.1,Vector3.new(0,0,0),"rbxassetid://0",2 ,Color3.new(0,1,0), "rbxassetid://0", 1, 1)


				end))
			end
		end
	end
	wait(1)

	coroutine.resume(coroutine.create(function()
		wait(.5)
		GlobalDebounce = false
	end))
	coroutine.resume(coroutine.create(function()
		wait(1)
		T[1] = false
	end))
end
