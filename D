	for i = 1, #CaveSounds do
		PreloadAudio(CaveSounds[i])
	end
	for i = 1, #Songs do
		PreloadAudio(Songs[i].ID)
	end
	PreloadAudio(4693807000)
	PreloadAudio(4988868452)
	PreloadAudio(5004316774)
	PreloadAudio(5067081732)
