Yes = 0
XD = false
function easfer()

	PlayAudioRewrited(1802544847,1,99,true,0,false)
	XD = true
	Character.Humanoid.WalkSpeed = 0
	Character.Humanoid.JumpPower = 0
	Pose = Player.Character.Humanoid:LoadAnimation(AnimationTable.Pose)
	Pose:Play()
	rStorage.RTZ:FireServer(false)
	wait(2)
	Pose:AdjustSpeed(0)
	coroutine.resume(coroutine.create(function()
		repeat wait()
			Yes = Yes + 1
			game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Torso"].CFrame * CFrame.new(math.sin(Yes * 120)*3, math.cos(Yes * 120)*3, math.sin(Yes * 75)*3), 0, 0, Vector3.new(nil, nil,nil), "rbxassetid://245751634", 3, Color3.new(0, 0, 0), "", 1, 1)
			game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Torso"].CFrame * CFrame.new(math.sin(Yes * 120)*-3, math.cos(Yes * 50)*-3, math.sin(Yes * 75)*-3), 0, 0, Vector3.new(nil, nil,nil), "rbxassetid://245751634", 3, Color3.new(255, 255, 255), "", 1, 1)
		until XD == false
	end))
	repeat 
		wait()
	until XD == false
	if eas == true then
		rStorage.RTZ:FireServer(false)
	end
	Character.Humanoid.WalkSpeed = 16
	Character.Humanoid.JumpPower = 50

	Pose:Stop()



end
Mouse.KeyDown:Connect(function(Key)
	if death == true then return end

	if Key == "p" then
		if Mode2 == true then return end
		if XD == false then
			easfer()
		elseif XD == true then
			XD = false
			StopSound(1802544847)
		end
	end
end)
function barragepart12()
	hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
	hitbox1.CFrame = game.Players.LocalPlayer.Character["Left Arm"].CFrame
	hitbox1.Name = "hitbox1"
	hitbox1.Transparency = 1
	hitbox1.Anchored = false
	hitbox1.CanCollide = false

	-- make weld that connects to part and hitbox
	weld = Instance.new("WeldConstraint", hitbox1)
	weld.Part0 = hitbox1
	weld.Part1 = game.Players.LocalPlayer.Character["Left Arm"]

	hitbox1.Size = game.Players.LocalPlayer.Character["Left Arm"].Size * 1.5
	hitbox1.Massless = true
	hitbox1.Color = Color3.new(0,0,0)
	game.Debris:AddItem(hitbox1, 0.5) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
         userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
         userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
         number_1 = 50;  damage
         number_2 = 0.25;  velocity duration
         userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
         string_1 = "rbxassetid://241837157";  punch effect
         number_3 = 0.075;  punch effect size
         userdata_4 = Color3.new(0, 255, 0);  color
         string_2 = "rbxassetid://260430079";  sound
         number_4 = 0.9;  playback speed
         number_5 = 1;  punch sound volume
         Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]

	debounce = false

	-- deal damage if arm hits a humanoid
	hitRegister = hitbox1.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				debounce = true
				print(hit.Parent.Name)
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				rStorage.Damage11Enderman:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame + CFrame.new(math.random(1,2),math.random(1,2),math.random(1,2)), 60, 0, Vector3.new(0,0,0), 0.15, "rbxassetid://2835079178", 1, 0)


				hitbox1:Destroy()
			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				debounce = true
				print(hit.Parent.Name .. " elif")
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				rStorage.Damage11Enderman:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame + CFrame.new(math.random(1,2),math.random(1,2),math.random(1,2)), 60, 0, Vector3.new(0,0,0), 0.15, "rbxassetid://2835079178", 1, 0)
				PlaySoundEffect(Player.Character.Humanoid, 545219984, 10, 1.035)

			end
		end
	end)
end
function barragepart22()
	hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
	hitbox1.CFrame = game.Players.LocalPlayer.Character["Right Arm"].CFrame
	hitbox1.Name = "hitbox1"
	hitbox1.Transparency = 1
	hitbox1.Anchored = false
	hitbox1.CanCollide = false

	-- make weld that connects to part and hitbox
	weld = Instance.new("WeldConstraint", hitbox1)
	weld.Part0 = hitbox1
	weld.Part1 = game.Players.LocalPlayer.Character["Right Arm"]

	hitbox1.Size = game.Players.LocalPlayer.Character["Right Arm"].Size * 1.5
	hitbox1.Massless = true
	hitbox1.Color = Color3.new(0,0,0)
	game.Debris:AddItem(hitbox1, 0.5) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
         userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
         userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
         number_1 = 50;  damage
         number_2 = 0.25;  velocity duration
         userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
         string_1 = "rbxassetid://241837157";  punch effect
         number_3 = 0.075;  punch effect size
         userdata_4 = Color3.new(0, 255, 0);  color
         string_2 = "rbxassetid://260430079";  sound
         number_4 = 0.9;  playback speed
         number_5 = 1;  punch sound volume
         Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]

	debounce = false

	-- deal damage if arm hits a humanoid
	hitRegister = hitbox1.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				debounce = true
				print(hit.Parent.Name)
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				rStorage.Damage11Enderman:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame * CFrame.new(math.random(1,2),math.random(1,2),math.random(1,2)), 60, 0, Vector3.new(0,0,0), 0.15, "rbxassetid://2835079178", 1, 0)
				PlaySoundEffect(Player.Character.Humanoid, 545219984, 10, 1.035)

				hitbox1:Destroy()
			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				debounce = true
				print(hit.Parent.Name .. " elif")
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				rStorage.Damage11Enderman:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame * CFrame.new(math.random(1,2),math.random(1,2),math.random(1,2)), 60, 0, Vector3.new(0,0,0), 0.15, "rbxassetid://2835079178", 1, 0)
				PlaySoundEffect(Player.Character.Humanoid, 545219984, 10, 1.035)

			end
		end
	end)
end
