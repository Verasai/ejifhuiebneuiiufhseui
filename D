    Mouse.KeyDown:Connect(function(Key)
    if Key == "b" then
   
			t()
			end
end)
]]
ts = game:GetService("TweenService")
cam = workspace.CurrentCamera

Time = 2  

Speeder = 1
if Speeder > 2 then return end
Humanoid = game.Players.LocalPlayer.Character:WaitForChild('Humanoid')
Runningi = false
function Sonic()

	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Runningi = true
		Player.Character.Humanoid.WalkSpeed = 0
		Player.Character.Humanoid.JumpPower = 0

		Der = Instance.new("Animation")
		Der.AnimationId = "rbxassetid://5225562211"

		Deer = BodyParts.Humanoid:LoadAnimation(Der)
		Deer:Play()
		wait(.5)
		Deer:AdjustSpeed(0)
		wait(1)
		Deer:Stop()
		PlayAudio(159335846, 7, 1)
		ts:Create(cam, TweenInfo.new(Time), {FieldOfView = 900}):Play()
		Player.Character.Humanoid.JumpPower = 150
		rStorage.Yell:FireServer()

		Boost = Instance.new("BodyVelocity")
		Boost.Name = "SonicRollVelocity"
		Boost.MaxForce = Vector3.new(100000, 0, 100000)
		Boost.P = math.huge
		Boost.Velocity = Player.Character.HumanoidRootPart.CFrame.lookVector * 85
		Boost.Parent = Player.Character.Torso
		RollAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Roll)

		RollAnim:Play(0.1, 1, 2.5)
		RollAnim:AdjustSpeed(1.5)
		Humanoid.Changed:Connect(function()
			if Runningi == true then
				if Humanoid.Jump and RollAnim.IsPlaying then
					RollAnim:Stop()
					Deeer = Instance.new("Animation")
					Deeer.AnimationId = "rbxassetid://5231652866"

					Deeeer = BodyParts.Humanoid:LoadAnimation(Deeer)
					Deeeer:Play()
					wait(0.4)
					Deeeer:AdjustSpeed(0)
					if Humanoid.FallingDown then
						wait(0.5)
						Deeeer:AdjustSpeed(1)
						Player.Character.Humanoid.WalkSpeed = 0
						wait(1)
						RollAnim:Play(0.1, 1, 2.5)
						RollAnim:AdjustSpeed(1.5)

					end




				end
			end
		end)


		repeat
			wait()
			Speeder = Speeder + 0.05
			game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Bright blue")) 
			Boost.Velocity = Player.Character.HumanoidRootPart.CFrame.lookVector * 85
		until CDown == false
		Boost:Destroy()
		activeTracks = Humanoid:GetPlayingAnimationTracks()
		for _,v in pairs(activeTracks) do
			v:Stop()
		end
		Runningi = false
		ts:Create(cam, TweenInfo.new(Time), {FieldOfView = 70}):Play()
		Player.Character.Humanoid.WalkSpeed = 16
		Player.Character.Humanoid.JumpPower = 50
	end)
end
Mouse.KeyDown:Connect(function(Key)
	if death == true then return end

	if Key == "c" then
		if 	Mode2 == true then return end

		CDown = true
		Sonic()
	end
end)
Mouse.KeyUp:Connect(function(Key)
	if death == true then return end

	if Key == "c" then
		if 	Mode2 == true then return end

		CDown = false

	end
end)
function Teleporter()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		rStorage.VampireDash:FireServer(BrickColor.new("Bright blue"))
		PlaySoundEffect(Player.Character.Humanoid,2642969814, 10, 1.125)

		Remotes.Damage:FireServer(Player.Character.Humanoid,Player.Character.HumanoidRootPart.CFrame,.2,0,Vector3.new(0,0,0),"rbxassetid://0",10,Color3.new(1,1,0),"rbxassetid://1091083826",0,0)
		wait()
		Player.Character:SetPrimaryPartCFrame(Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -20))
		wait()
		Remotes.Damage:FireServer(Player.Character.Humanoid,Player.Character.HumanoidRootPart.CFrame,.2,0,Vector3.new(0,0,0),"rbxassetid://0",10,Color3.new(1,1,0),"rbxassetid://1091083826",0,0)

	end)
end
Mouse.KeyDown:Connect(function(Key)
	if death == true then return end

	if Key == 'v' then
		if 	Mode2 == true then return end

		Teleporter()
	end
end)
Objs = {}

