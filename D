local SpikeHolder = false

function spikes2()
	RecordPoint = BodyParts.HumanoidRootPart.Position
	AxeKick = Instance.new("Animation")
	AxeKick.AnimationId = "rbxassetid://3681052896"

	AxeKickTrack = BodyParts.Humanoid:LoadAnimation(AxeKick)
	AxeKickTrack:Play()
	wait(1)
	SpikeHolder = true
	--CurrentCamera.CameraType = Enum.CameraType.Scriptable
	Model.Parent = chr


	knife = Instance.new("Part")
	knife.Anchored = false
	knife.CanCollide = false
	knife.Massless = true
	knife.Transparency = 1
	knife.Size = Vector3.new(30,30,30)
	knife.CFrame = hrp.CFrame * CFrame.new(0,10,0)
	knife.Parent = Model

	boopyve = Instance.new("BodyVelocity")
	boopyve.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
	boopyve.P = math.huge
	boopyve.Parent = knife
	boopyve.Name = "Force"

	coroutine.resume(coroutine.create(function()
		ProjectileAuraSetter = 0
		repeat

			coroutine.resume(coroutine.create(function()
				ProjectileAuraSetter = ProjectileAuraSetter + 1
				--	    CurrentCamera.CameraSubject = knife
				-- catedStorage.Damage12Enderman:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*11.25, math.cos(ProjectileAuraSetter * 50)*11.25, math.sin(ProjectileAuraSetter * 75)*11.25) , 0, 0, nil, 0.5, "rbxassetid://2835079178", 1, 0)
				game.ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*21.25, math.cos(ProjectileAuraSetter * 50)*21.25, math.sin(ProjectileAuraSetter * 75)*21.25), 1, 0,Vector3.new(0, 0, 0), "rbxassetid://", 5, Color3.new(255, 255, 255), "rbxassetid://367499850", 0, 0)
				game.ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*-21.25, math.cos(ProjectileAuraSetter * 50)*-21.25, math.sin(ProjectileAuraSetter * 75)*-21.25), 1, 0,Vector3.new(255, 255, 255), "rbxassetid://", 5, Color3.new(255, 255, 255), "rbxassetid://367499850", 0, 0)
				--rs.Damage:FireServer(human, knife.CFrame * CFrame.new(0,0,0), .3, 0,Vector3.new(0, 0, 0), "rbxassetid://", 10, Color3.new(255, 0, 0), "", 1, 1)
				--         		game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, knife.CFrame * CFrame.new(0,0,1.75), 0, 0, nil, 0.5, "", 1, 0)

			end))
			boopyve.Velocity = knife.CFrame.lookVector * 250
			coroutine.resume(coroutine.create(function()
				knife.CFrame = CFrame.lookAt(knife.Position, Mouse.Hit.p)
			end))
			wait()
		until SpikeHolder == false
		-- CurrentCamera.CameraSubject = Player.Character.Head
		knife:Destroy()
	end))

	debounce = false

	-- deal damage if arm hits a humanoid
	hitRegister = knife.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
				game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(hit.Parent.Humanoid)
				game:GetService("ReplicatedStorage").Damage11:FireServer(human, hit.Parent.HumanoidRootPart.CFrame*CFrame.new(math.random(-5,5),math.random(-1,5),math.random(-5,5)), 0, 0, Vector3.new(0, 0, 0), .05, "", 1, 1)

			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Parent.Humanoid)
				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
				game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(hit.Parent.Humanoid)
				game:GetService("ReplicatedStorage").Damage11:FireServer(human, hit.Parent.Parent.HumanoidRootPart.CFrame*CFrame.new(math.random(-5,5),math.random(-1,5),math.random(-5,5)), 0, 0, Vector3.new(0, 0, 0), .05, "", 1, 1)

			end
		end
	end)

	--HitBox(Player.Character["Torso"], 75.5, 0.2, 0.3, 99, Player.Character.Torso.CFrame.lookVector * 175 + Vector3.new(0, 45, 0), 0.25, "rbxassetid://3240049847", false, true, 10.35, true)
end
bep = false
AuraSetter3 = 0
function blast()
	bep = true
	PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.CharageIntro)
	--3477214701, 7, 1.1)
	PunchAnim:Play(0.1, 1, 1)
	PunchAnim:AdjustSpeed(1)

	wait(0.5)
	PlayAudio(1370232812, 7, 1.1)
	wait(0.2)
	PunchAnim:AdjustSpeed(0)
	repeat
		wait(0.1)
		AuraSetter2 = AuraSetter2 + 1

		rStorage.Damage12Sans:FireServer(Player.Character.Humanoid, Player.Character["Left Arm"].CFrame * CFrame.new(0.6,-0.5,-0.4), 0, 0, nil, 0.015, "rbxassetid://1693499499", 1, 0)

	until bep == false
	PunchAnim:Stop(.3)	
	E = Player.Character.Humanoid:LoadAnimation(AnimationTable.Blast)
	--3477214701, 7, 1.1)
	E:Play(0.1, 1, 1)
	E:AdjustSpeed(1)
	wait(.3)
	E:AdjustSpeed(0)
	PlaySoundEffect(Player.Character.Humanoid,1392670703,99,1)
	ABDSound.StopAudio(1370232812)
	Model.Parent = chr

	knifeass = Instance.new("Part")
	knife = knifeass:Clone()
	knife.Anchored = false
	knife.CanCollide = false
	knife.Massless = true
	knife.Transparency = 1
	knife.Size = Vector3.new(20,20,20)
	knife.CFrame = hrp.CFrame * CFrame.new(0,0,0)
	knife.Parent = Model

	boopyve = Instance.new("BodyVelocity")
	boopyve.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
	boopyve.P = math.huge
	boopyve.Velocity = knife.CFrame.lookVector * 105
	boopyve.Parent = knife
	boopyve.Name = "Force"

	coroutine.resume(coroutine.create(function()
		game.Debris:AddItem(knife, 5)

		wait(5)
		knifeass:Destroy()
		Model.Parent = game.Workspace
	end))
	coroutine.resume(coroutine.create(function()
		wait(2)
		E:Stop()
	end))

	coroutine.resume(coroutine.create(function()
		ProjectileAuraSetter = 0
		repeat
			wait()
			AuraSetter3 = AuraSetter3 + 1
			game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(AuraSetter3 * 50)*-1, math.cos(AuraSetter3 * 50)*-1, math.sin(AuraSetter3 * 50)*-1)* CFrame.new(0,-0.75,0), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)

			game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(AuraSetter3 * 50)*1, math.cos(AuraSetter3 * 50)*1, math.sin(AuraSetter3 * 50)*1)* CFrame.new(0,-0.75,0), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)
			game:GetService("ReplicatedStorage").Damage12Sans:FireServer(human, knife.CFrame * CFrame.new(0,0,-10) , 1, 0, Vector3.new(0, 0, 0), .15, "", 1, 1)

		until Model.Parent ~= chr
	end))

	debounce = false

	-- deal damage if arm hits a humanoid
	hitRegister = knife.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)

				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)


			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)

				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)


			end
		end
	end)
end
