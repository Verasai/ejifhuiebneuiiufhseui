travdis = 0
rs = game:GetService("ReplicatedStorage")
function spikes()

	AxeKick = Instance.new("Animation")
	AxeKick.AnimationId = "rbxassetid://3681052896"

	AxeKickTrack = BodyParts.Humanoid:LoadAnimation(AxeKick)
	AxeKickTrack:Play()
	wait(1)
	knife = Instance.new("Part")
	knife.Anchored = false
	knife.CanCollide = false
	knife.Massless = true
	knife.Transparency = 1
	knife.Size = Vector3.new(100,100,100)
	knife.CFrame = hrp.CFrame * CFrame.new(0,0,0)
	knife.Parent = Model

	boopyve = Instance.new("BodyVelocity")
	boopyve.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
	boopyve.P = math.huge
	boopyve.Parent = knife
	boopyve.Name = "Force"

	coroutine.resume(coroutine.create(function()
		ProjectileAuraSetter = 0
		repeat
			travdis = travdis + 1
			coroutine.resume(coroutine.create(function()
				ProjectileAuraSetter = ProjectileAuraSetter + 1
				--	rs.Damage:FireServer(human, knife.CFrame * CFrame.new(0,-6,1.75), 0, 0,Vector3.new(0, 0, 0), "rbxassetid://", 1, Color3.new(255, 255, 0), "", 1, 1)
				game.ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*21.25, math.cos(ProjectileAuraSetter * 50)*21.25, math.sin(ProjectileAuraSetter * 75)*21.25), 1, 0,Vector3.new(0, 0, 0), "rbxassetid://", 5, Color3.new(1, 0, 0), "rbxassetid://367499850", 0, 0)
				game.ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, knife.CFrame * CFrame.new(math.sin(ProjectileAuraSetter * 120)*-21.25, math.cos(ProjectileAuraSetter * 50)*-21.25, math.sin(ProjectileAuraSetter * 75)*-21.25), 1, 0,Vector3.new(0, 0, 0), "rbxassetid://", 5, Color3.new(1,0,0), "rbxassetid://367499850", 0, 0)

			end))
			boopyve.Velocity = knife.CFrame.lookVector * 100
			coroutine.resume(coroutine.create(function()
				for i,v in pairs(game.Workspace.Entities:GetChildren()) do
					if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
						if (knife.Position - v.HumanoidRootPart.Position).Magnitude < 80 then
							--	knife.CFrame = CFrame.lookAt(knife.Position, v.HumanoidRootPart.Position)
							wait(9e999)
						end
					end
				end
			end))
			wait()
		until travdis == 50 or travdis > 50
		travdis = 0
		knife:Destroy()
	end))

	debounce = false

	-- deal damage if arm hits a humanoid
	hitRegister = knife.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
				game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(hit.Parent.Humanoid)
				game:GetService("ReplicatedStorage").Damage11:FireServer(human, hit.Parent.HumanoidRootPart.CFrame*CFrame.new(math.random(-5,5),math.random(-1,5),math.random(-5,5)), 0, 0, Vector3.new(0, 0, 0), .05, "", 1, 1)

			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Parent ~= chr then
			if debounce == false then
				debounce = true
				delay(.01, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Parent.Humanoid)
				game.ReplicatedStorage.Knock:FireServer(hit.Parent.Parent.Humanoid)
				rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)

				game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(hit.Parent.Humanoid)
				game:GetService("ReplicatedStorage").Damage11:FireServer(human, hit.Parent.Parent.HumanoidRootPart.CFrame*CFrame.new(math.random(-5,5),math.random(-1,5),math.random(-5,5)), 0, 0, Vector3.new(0, 0, 0), .05, "", 1, 1)

			end
		end
	end)
	--HitBox(Player.Character["Torso"], 75.5, 0.2, 0.3, 99, Player.Character.Torso.CFrame.lookVector * 175 + Vector3.new(0, 45, 0), 0.25, "rbxassetid://3240049847", false, true, 10.35, true)
end

Mouse.KeyDown:Connect(function(Key)
	if death == true then return end

	if Key == 'h' then
		if 	Mode2 == true then return end

		spikes()
	end
end)

function Drip(Target)
	rStorage.VampireDash:FireServer(BrickColor.new("Bright blue"))
	PlaySoundEffect(Player.Character.Humanoid,2642969814, 10, 1.125)

	Remotes.Damage:FireServer(Player.Character.Humanoid,Player.Character.HumanoidRootPart.CFrame,.2,0,Vector3.new(0,0,0),"rbxassetid://0",10,Color3.new(1,1,0),"rbxassetid://1091083826",0,0)
	wait()
	Vanish = Instance.new("Animation")
	Vanish.AnimationId = "rbxassetid://5177938766"

	Vanisher = BodyParts.Humanoid:LoadAnimation(Vanish)
	Vanisher:Play()
	Vanisher.TimePosition = 0.1
	speaker = game.Players.LocalPlayer
	Clip = false
	wait(0.1)
	function NoclipLoop()
		if Clip == false and speaker.Character ~= nil then
			for _, child in pairs(speaker.Character:GetDescendants()) do
				if child:IsA("BasePart") and child.CanCollide == true and child.Name ~= floatName then
					child.CanCollide = false
				end
			end
		end
	end
	Noclipping = game:GetService('RunService').Stepped:Connect(NoclipLoop)
	rStorage.Damage:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame, 1, 8, Vector3.new(0,0,0), "rbxassetid://5482711860", 0.5, Color3.new(175, 175, 175), "rbxassetid://638861091", 0, 0)
	wait(7)
	Vanisher:Stop()
	if Noclipping then
		Noclipping:Disconnect()
	end
	Clip = true
	wait()
	Lines = {"Nothing Personal kid.", "Cry about it.", "NIGGER", "I FUCK YOUR MOM", "I HATE ALL NIGGER"}
	H = Instance.new("Animation")
	H.AnimationId = "rbxassetid://5668309864"

	He = BodyParts.Humanoid:LoadAnimation(H)
	He:Play()
	He:AdjustSpeed(0.1)
	He.TimePosition = 0.2
	for i = 1, 1 do
		Player.Character.HumanoidRootPart.CFrame = Target.HumanoidRootPart.CFrame * CFrame.new(0, 5, 3.65)
		rStorage.Damage:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame, 1, 3, Vector3.new(0,0,0), "rbxassetid://5482711860", 0.5, Color3.new(175, 175, 175), "rbxassetid://638861091", 0, 0)


		rStorage.Damage12Sans:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Head.CFrame, 1e-320, 0, Vector3.new(0, 0, 0), 0.3, "rbxassetid://2642969814", 1, 11)

	end
	wait(2)
	He:AdjustSpeed(2)
	rStorage.DoppioSlam:FireServer()
	rStorage.Damage12Enderman:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Head.CFrame, 1e-320, 0, Vector3.new(0, 0, 0), 0.6, "rbxassetid://2642969814", 1, 11)
	game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(Target:FindFirstChildOfClass("Humanoid"))
	for i = 1,50 do
		game.ReplicatedStorage.HamonSword:FireServer(Target:FindFirstChildOfClass("Humanoid"), 1, math.huge, Vector3.new(0,math.huge,0))

	end


end
Mouse.KeyDown:Connect(function(Key)
	if death == true then return end

	if Key == "l" then
		if 	Mode2 == true then return end

		if Mouse.Target and Mouse.Target.Parent then
			Character = nil
			if Mouse.Target.Parent:IsA("Model") and Mouse.Target.Parent.Name == "Stand" then
				Character = Mouse.Target.Parent.Parent
			elseif Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") then
				Character = Mouse.Target.Parent
			end
			if Character then
				Drip(Character)
			end
		end
	end
end)
blackholer = false

function BlackHole()
	coroutine.resume(coroutine.create(function()
		repeat wait()
		until blackholer == false
	end))
end
c = {}
stop = {}
function epic(amountOfTime)
	t = tick()
	while tick()-t < amountOfTime do
		game:GetService("RunService").Heartbeat:Wait()
	end
end
StopPlay = false
woopID = 0
function PlaySoundImprove(id,volume,pitch,loop)
	c[id] = 0
	stop[id] = false

	s = Instance.new("Sound")
	s.SoundId = "rbxassetid://"..id
	s.Parent = workspace
	s.Volume = 0
	repeat wait() until s.TimeLength > 0
	maxtime = s.TimeLength
	s:Destroy()
	coroutine.wrap(function()
		workspace.Effects.ChildAdded:Connect(function(d)
			game.RunService.Heartbeat:Wait()
			for i,v in pairs(workspace.Effects:GetDescendants()) do
				if v.Parent == d and v:IsA("Sound") and v.SoundId == "rbxassetid://"..id and stop[id] == false then
					stop[id] = true
					v.TimePosition = c[id]
					if v.TimePosition >= maxtime and loop == false then 
						return 
					elseif loop == true and v.TimePosition >= maxtime then
						maxtime = 0
					end
				end
			end
		end)
		while loop and StopPlay == false do
			for i = 1,maxtime do
				game:GetService("ReplicatedStorage").Damage11Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid,game.Players.LocalPlayer.Character.Head.CFrame * CFrame.new(0,-5,0),0,0, Vector3.new(0, 0, 0),9e999, "rbxassetid://"..id,pitch,volume)
				c[id] = c[id] + 1
				epic(1)
				stop[id] = false
				if StopPlay and woopID == id then
					break
				end
			end


			c[id] = 0
		end
	end)()
end
function StopAudioImprove(ID)

	woopID = ID
	StopPlay = true 
	wait(.1)
	woopID = 0 
	StopPlay = false

end

