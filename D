	end
end)
function HeartHitBoxer()
	hitbox1.CFrame = game.Players.LocalPlayer.Character["Left Arm"].CFrame
	hitbox1.Name = "hitbox1"
	hitbox1.Transparency = 1
	hitbox1.Anchored = false
	hitbox1.CanCollide = false

	-- make weld that connects to part and hitbox
	weld = Instance.new("WeldConstraint", hitbox1)
	weld.Part0 = hitbox1
	weld.Part1 = game.Players.LocalPlayer.Character["Left Arm"]

	hitbox1.Size = game.Players.LocalPlayer.Character["Left Arm"].Size * 25
	hitbox1.Massless = true
	hitbox1.Color = Color3.new(0,0,0)
	game.Debris:AddItem(hitbox1, 0.5) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
         userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
         userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
         number_1 = 50;  damage
         number_2 = 0.25;  velocity duration
         userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
         string_1 = "rbxassetid://241837157";  punch effect
         number_3 = 0.075;  punch effect size
         userdata_4 = Color3.new(0, 255, 0);  color
         string_2 = "rbxassetid://260430079";  sound
         number_4 = 0.9;  playback speed
         number_5 = 1;  punch sound volume
         Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]

	debounce = false

	-- deal damage if arm hits a humanoid
	hitRegister = hitbox1.Touched:Connect(function(hit)
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				debounce = true
				print(hit.Parent.Name)
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				for i = 1,20 do
					game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
					game.ReplicatedStorage.HamonSword:FireServer(hit.Parent.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
					rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
					rStorage.Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 60, 10, Vector3.new(1000,1000,1000), 15, "rbxassetid://2835079178", 1, 0)
				end

				hitbox1:Destroy()
			end
		elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
			if debounce == false then
				debounce = true
				print(hit.Parent.Name .. " elif")
				delay(.001, function() debounce = false end)
				game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
				for i = 1,20 do
					game.ReplicatedStorage.Berserk:FireServer(hit.Parent.Humanoid)
					game.ReplicatedStorage.HamonSword:FireServer(hit.Parent.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
					rStorage.BurnDamage:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
					rStorage.Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 60, 10, Vector3.new(1000,1000,1000), 15, "rbxassetid://2835079178", 1, 0)
				end

			end
		end
	end)
end
AuraSetter2 = 0
function HeartPunch()
	Smash = Player.Character.Humanoid:LoadAnimation(AnimationTable.HeartBreaker)
	Smash:Play()
	wait(.3)
	Smash:AdjustSpeed(0)
	AuraSetter2 = 0
	repeat
		wait()
		AuraSetter2 = AuraSetter2 + 1
		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Left Arm"].CFrame * CFrame.new(0,-0.75,0), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)

		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Left Arm"].CFrame * CFrame.new(math.sin(AuraSetter2 * 50)*-1, math.cos(AuraSetter2 * 50)*-1, math.sin(AuraSetter2 * 50)*-1)* CFrame.new(0,-0.75,0), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)

		game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Left Arm"].CFrame * CFrame.new(math.sin(AuraSetter2 * 50)*1, math.cos(AuraSetter2 * 50)*1, math.sin(AuraSetter2 * 50)*1)* CFrame.new(0,-0.75,0), 0, 0, Vector3.new(nil,nil,nil), "rbxassetid://245751634", 3, Color3.new(191, 183, 177), "", 1, 1)

	until AuraSetter2 == 100
	Smash:AdjustSpeed(2)
	wait(0.1)
	SmashHitBox()
	game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-2,-2,-2), Vector3.new(-1,-1,-1), BrickColor.new("Pastel Blue"))
