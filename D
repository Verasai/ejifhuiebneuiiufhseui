function UltraBarrage()
	PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Barrage)
	duration = .2
	speed = PunchAnim.Length / duration
	lenght = duration / PunchAnim.Length
	print(duration / PunchAnim.Length)
	Barraging = true

	coroutine.resume(coroutine.create(function()
		repeat

			wait(duration / PunchAnim.Length)
			PunchAnim:Play()
			PunchAnim:AdjustSpeed(PunchAnim.Length / duration)
		until Barraging == false
	end))
	repeat wait(0.025)


		PlaySoundEffect(Player.Character.Humanoid, 200632136, 10, 1.035)

		a = math.random(1,3)
		if a == 1 then
			barragepart1()
		elseif a == 2 then
			barragepart2()
		elseif a == 3 then
			barragepart3()
		end
	until Barraging == false
	PunchAnim:Stop()
end
Mouse.KeyDown:Connect(function(ke)
	if death == true then return end

	if ke == "e" then
		if 	Mode2 == true then return end

		UltraBarrage()
	end
end)
Mouse.KeyUp:Connect(function(ke)
	if death == true then return end

	if ke == "e" then

		Barraging = false
	end
end)
function Teleport()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		E = Player.Character.Humanoid:LoadAnimation(AnimationTable.Transition)
		E:Play()
		E:AdjustSpeed(0.5)
		wait(.5)
		E:AdjustSpeed(0)
		rStorage.VampireDash:FireServer(BrickColor.new("Transparency"))
		PlaySoundEffect(Player.Character.Humanoid,2642969814, 10, 1.125)

		Remotes.Damage:FireServer(Player.Character.Humanoid,Player.Character.HumanoidRootPart.CFrame,.2,0,Vector3.new(0,0,0),"rbxassetid://0",10,Color3.new(0,0,0),"rbxassetid://1091083826",0,0)
		wait()
		Player.Character:SetPrimaryPartCFrame(CFrame.new(Mouse.Hit.p + Vector3.new(0, 3.5, 0)))

		wait(.1)
		rStorage.VampireDash:FireServer(BrickColor.new("Transparency"))

		Remotes.Damage:FireServer(Player.Character.Humanoid,Player.Character.HumanoidRootPart.CFrame,.2,0,Vector3.new(0,0,0),"rbxassetid://0",10,Color3.new(0,0,0),"rbxassetid://1091083826",0,0)
		wait(.1)
		E:Stop()

	end)
end
Mouse.KeyDown:Connect(function(Key)
	if death == true then return end

	if Key == 'q' then

		Teleport()
	end
end)
